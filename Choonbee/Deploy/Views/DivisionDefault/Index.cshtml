@model IEnumerable<Choonbee.Models.DivisionDefault>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
@if(Model.Count() != 0){
    <table>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.FriendlyId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.RankGroup.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Genders)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AgeGroup.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DivisionType.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DateEntered)
            </th>
            <th></th>
        </tr>


    @foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FriendlyId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.RankGroup.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Genders)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AgeGroup.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DivisionType.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DateEntered)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id=item.DivisionDefaultId }) |
                @Html.ActionLink("Details", "Details", new { id=item.DivisionDefaultId }) |
                @Html.ActionLink("Delete", "Delete", new { id=item.DivisionDefaultId })
            </td>
        </tr>
    }
    </table>
}
else
{
 <p>No default divisions.</p>   
}

<a href="/">Home..</a>